{
    "name": "elgentos/magento2-openkvk",
    "description": "Extension to load KVK (Dutch Chamber of Commerce) data into Magento",
    "license": [
        "proprietary"
    ],
    "type": "magento2-module",
    "authors": [
        {
            "name": "elgentos",
            "email": "info@elgentos.nl"
        }
    ],
    "require": {
        "magento/framework": "^103.0",
        "magento/module-customer": "^103.0",
        "magento/module-eav": "^102.1",
        "magento/module-store": "^101.1",
        "webonyx/graphql-php": "^14.5"
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Elgentos\\OpenKvk\\": "src/"
        },
        "files": [
            "src/registration.php"
        ]
    },
    "require-dev": {
        "mediact/testing-suite": "@stable",
        "mediact/coding-standard-magento2": "@stable"
    },
    "autoload-dev": {
        "psr-4": {
            "Elgentos\\OpenKvk\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/.gitignore",
            "/grumphp.yml",
            "/pdepend.xml",
            "/phpstan.neon",
            "/phpunit.xml",
            "/phpcs.xml",
            "/phpmd.xml",
            "/package.json",
            "/.eslintrc.json",
            "/.eslintignore",
            "/tests"
        ]
    },
    "config": {
        "sort-packages": true,
        "mediact-testing-suite": {
            "type": "magento2"
        }
    }
}
